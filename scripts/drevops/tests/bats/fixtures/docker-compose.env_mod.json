{
  "name": "the_matrix",
  "services": {
    "chrome": {
      "command": null,
      "depends_on": {
        "cli": {
          "condition": "service_started"
        }
      },
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "image": "selenium/standalone-chrome",
      "labels": {
        "lagoon.type": "none"
      },
      "networks": {
        "default": null
      },
      "shm_size": "1073741824",
      "volumes": [
        {
          "type": "volume",
          "source": "app",
          "target": "/myapp",
          "volume": {}
        },
        {
          "type": "volume",
          "source": "files",
          "target": "/myapp/docroot/sites/default/files",
          "volume": {}
        }
      ]
    },
    "cli": {
      "build": {
        "context": "FIXTURE_CUR_DIR",
        "dockerfile": ".docker/cli.dockerfile",
        "args": {
          "GITHUB_TOKEN": "",
          "WEBROOT": "docroot"
        }
      },
      "command": null,
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "image": "the_matrix",
      "labels": {
        "lagoon.persistent": "/myapp/docroot/sites/default/files/",
        "lagoon.persistent.name": "nginx-php",
        "lagoon.type": "cli-persistent"
      },
      "networks": {
        "default": null
      },
      "ports": [
        {
          "mode": "ingress",
          "target": 35729,
          "protocol": "tcp"
        }
      ],
      "user": "root",
      "volumes": [
        {
          "type": "volume",
          "source": "app",
          "target": "/myapp",
          "volume": {}
        },
        {
          "type": "volume",
          "source": "files",
          "target": "/myapp/docroot/sites/default/files",
          "volume": {}
        }
      ]
    },
    "mariadb": {
      "build": {
        "context": "FIXTURE_CUR_DIR",
        "dockerfile": ".docker/mariadb.dockerfile",
        "args": {
          "IMAGE": "myorg/my_db_image"
        }
      },
      "command": null,
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "labels": {
        "lagoon.type": "mariadb"
      },
      "networks": {
        "default": null
      },
      "ports": [
        {
          "mode": "ingress",
          "target": 3306,
          "protocol": "tcp"
        }
      ]
    },
    "nginx": {
      "build": {
        "context": "FIXTURE_CUR_DIR",
        "dockerfile": ".docker/nginx-drupal.dockerfile",
        "args": {
          "CLI_IMAGE": "the_matrix",
          "WEBROOT": "docroot"
        }
      },
      "command": null,
      "depends_on": {
        "cli": {
          "condition": "service_started"
        }
      },
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "labels": {
        "lagoon.name": "nginx-php",
        "lagoon.persistent": "/myapp/docroot/sites/default/files/",
        "lagoon.persistent.class": "slow",
        "lagoon.type": "nginx-php-persistent"
      },
      "networks": {
        "amazeeio-network": null,
        "default": null
      },
      "user": "1000",
      "volumes": [
        {
          "type": "volume",
          "source": "app",
          "target": "/myapp",
          "volume": {}
        },
        {
          "type": "volume",
          "source": "files",
          "target": "/myapp/docroot/sites/default/files",
          "volume": {}
        }
      ]
    },
    "php": {
      "build": {
        "context": "FIXTURE_CUR_DIR",
        "dockerfile": ".docker/php.dockerfile",
        "args": {
          "CLI_IMAGE": "the_matrix"
        }
      },
      "command": null,
      "depends_on": {
        "cli": {
          "condition": "service_started"
        }
      },
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "labels": {
        "lagoon.name": "nginx-php",
        "lagoon.persistent": "/myapp/docroot/sites/default/files/",
        "lagoon.persistent.class": "slow",
        "lagoon.type": "nginx-php-persistent"
      },
      "networks": {
        "default": null
      },
      "user": "1000",
      "volumes": [
        {
          "type": "volume",
          "source": "app",
          "target": "/myapp",
          "volume": {}
        },
        {
          "type": "volume",
          "source": "files",
          "target": "/myapp/docroot/sites/default/files",
          "volume": {}
        }
      ]
    },
    "redis": {
      "command": null,
      "entrypoint": null,
      "image": "uselagoon/redis-6:23.2.0",
      "labels": {
        "lagoon.type": "redis"
      },
      "networks": {
        "default": null
      }
    },
    "solr": {
      "build": {
        "context": "FIXTURE_CUR_DIR",
        "dockerfile": ".docker/solr.dockerfile",
        "args": {
          "CLI_IMAGE": "the_matrix"
        }
      },
      "command": null,
      "depends_on": {
        "cli": {
          "condition": "service_started"
        }
      },
      "entrypoint": null,
      "environment": {
        "CI": "true",
        "DREVOPS_LOCALDEV_URL": "the_matrix.docker.amazee.io",
        "DREVOPS_REDIS_ENABLED": "1",
        "DRUPAL_SHIELD_PASS": "passw",
        "DRUPAL_SHIELD_USER": "jane",
        "LAGOON_ENVIRONMENT_TYPE": "development",
        "LAGOON_ROUTE": "the_matrix.docker.amazee.io",
        "SSMTP_MAILHUB": "false",
        "XDEBUG_ENABLE": "1"
      },
      "labels": {
        "lagoon.type": "solr"
      },
      "networks": {
        "default": null
      },
      "ports": [
        {
          "mode": "ingress",
          "target": 8983,
          "protocol": "tcp"
        }
      ],
      "volumes": [
        {
          "type": "volume",
          "source": "search",
          "target": "/var/solr",
          "volume": {}
        }
      ]
    },
    "wait_dependencies": {
      "command": [
        "mariadb:3306"
      ],
      "depends_on": {
        "cli": {
          "condition": "service_started"
        },
        "mariadb": {
          "condition": "service_started"
        }
      },
      "entrypoint": null,
      "image": "dadarek/wait-for-dependencies",
      "labels": {
        "lagoon.type": "none"
      },
      "networks": {
        "default": null
      }
    }
  },
  "networks": {
    "amazeeio-network": {
      "name": "amazeeio-network",
      "ipam": {},
      "external": true
    },
    "default": {
      "name": "the_matrix_default",
      "ipam": {},
      "external": false
    }
  },
  "volumes": {
    "app": {
      "name": "the_matrix_app",
      "external": false
    },
    "files": {
      "name": "the_matrix_files",
      "external": false
    },
    "search": {
      "name": "the_matrix_search",
      "external": false
    }
  }
}
